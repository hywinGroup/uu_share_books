.search-keyword {
    padding: 30rpx;
}

.search-keyword-title {
    padding-left: 20rpx;
    margin-top: 40rpx;
    margin-bottom: 20rpx;
    font-size: 30rpx;
    border-left: 10rpx solid #47a86c;
}

.search-keyword-item {
    display: inline-block;
    padding: 10rpx;
    border: 1px solid #ccc;
    border-radius: 10rpx;
    background-color: #fbfbfb;
    margin: 0 10rpx 10rpx 0;
    color: #555;
    font-size: 24rpx;
}
.searchList{
  text-align: center;
}
.searchList .search-keyword-item{
  margin: 0 20rpx 10rpx 0;
   padding: 10rpx 20rpx;
}